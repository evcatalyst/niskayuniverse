{
  "name": "service-line-markers",
  "version": "1.0.0",
  "description": "Service-line marker design system and control panel with GitHub Pages hosting and Actions backend",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run --passWithNoTests",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:accessibility": "pa11y-ci",
    "convert": "node scripts/convert-csv-to-json.js",
    "geocode": "node scripts/geocode-addresses.mjs",
    "geocode:ci": "node scripts/geocode-addresses-ci.mjs",
    "geocode:nys": "node scripts/geocode-nys-gis.mjs",
    "ingest:pdf": "node scripts/ingest-pdf.mjs",
    "generate:pmtiles": "node scripts/generate-pmtiles.mjs"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "pa11y-ci": "^3.0.0",
    "pdf-parse": "^1.1.1",
    "playwright": "^1.40.0",
    "@playwright/test": "^1.40.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "zod": "^3.22.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}